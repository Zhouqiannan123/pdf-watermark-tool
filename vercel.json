{
  "version": 2,
  "builds": [
    {
      "src": "web_watermark_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_watermark_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "web_watermark_app.py": {
      "maxDuration": 60
    }
  }
}
